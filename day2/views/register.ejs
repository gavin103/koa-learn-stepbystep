<%- include('header'); -%>
<div class="box-wrap" style="height: 240px;">
  <form>
    <div class="form-group">
      <input id="email" type="email" class="form-control" placeholder="邮箱">
    </div>
    <div class="form-group">
      <input id="password" type="password" class="form-control" placeholder="密码">
    </div>
    <div class="form-group">
      <input id="repeatPassword" type="password" class="form-control" placeholder="确认密码">
    </div>
    <div class="form-inline">
      <a href="/login">已有账号，去登录？</a>
      <button id="submitBtn" type="submit" class="btn btn-primary">提交</button>
    </div>

  </form>
</div>
<%- include('footer'); -%>

<script src="/js/register.js"></script>